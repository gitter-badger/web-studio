{
  "include": [
    "./ws/**/*"
  ],
  "compilerOptions": {
    "outDir": "./app/dist/",
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "dom",
      "es5",
      "es2015.promise"
    ],
    "strict": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "module": "es2015",
    "moduleResolution": "node"
  }
}