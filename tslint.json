{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true
    ],
    "interface-name": [
      false
    ],
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "case-insensitive"
      }
    ],
    "semicolon": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}